{
  "name": "cwt-meetings-bot",
  "version": "0.0.0-alpha",
  "author": "Tor E Hagemann <tohagema@cisco.com>",
  "description": "Cisco Webex Teams bot to help schedule meetings",
  "homepage": "https://github.com/hagemt/ciscospark-calendarbot#readme",
  "license": "UNLICENSED",
  "main": "mvp/index.js",
  "private": true,
  "bugs": {
    "url": "https://github.com/hagemt/ciscospark-calendarbot/issues"
  },
  "dependencies": {
    "@koa/cors": "2.2.1",
    "boom": "7.2.0",
    "bunyan": "1.8.12",
    "chrono-node": "1.3.5",
    "ciscospark-webhook-validator": "1.0.0",
    "co-body": "5.2.0",
    "config": "1.30.0",
    "dataloader": "1.4.0",
    "googleapis": "29.0.0",
    "http-shutdown": "1.2.0",
    "koa": "2.5.1",
    "koa-omnibus": "0.9.3",
    "koa-router": "7.4.0",
    "koa-static": "4.0.2",
    "lodash": "4.17.10",
    "moment": "2.22.1",
    "ngrok": "3.0.1",
    "node-fetch": "2.1.2",
    "stopwords": "0.0.9",
    "uuid": "3.2.1"
  },
  "devDependencies": {
    "eslint": "4.19.1",
    "eslint-plugin-import": "2.11.0",
    "eslint-plugin-mocha": "5.0.0",
    "eslint-plugin-node": "6.0.1",
    "eslint-plugin-react": "7.7.0",
    "mocha": "5.1.1",
    "nyc": "11.7.1",
    "react": "16.3.2",
    "react-dom": "16.3.2",
    "react-scripts": "1.1.4",
    "supertest": "3.0.0"
  },
  "directories": {
    "test": "test"
  },
  "engines": {
    "node": ">=8.2.0"
  },
  "keywords": [
    "cisco",
    "webex",
    "teams",
    "bot",
    "schedule",
    "meeting",
    "collaboration",
    "gsuite",
    "outlook",
    "calendar"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hagemt/ciscospark-calendarbot.git"
  },
  "scripts": {
    "precoverage": "nyc -- npm run mocha",
    "coverage": "nyc report --reporter=html",
    "postcoverage": "nyc check-coverage --lines 0",
    "predocker": "docker build --file ./config/mvp.Dockerfile --tag cwt-meetings-bot/mvp .",
    "docker": "docker run --detach -p 8080:8080 cwt-meetings-bot/mvp # later: docker rm $CONTAINER",
    "postdocker": "node scripts/browser.window.js http://localhost:8080/v0/ping https://web.ciscospark.com/",
    "eslint": "eslint .eslintrc.js config mvp scripts test --report-unused-disable-directives",
    "mocha": "NODE_ENV=test mocha test/mvp.js",
    "mvp": "node mvp | bunyan",
    "prereport": "npm run eslint -- --fix",
    "report": "npm run coverage",
    "postreport": "npm run eslint -- --max-warnings 0",
    "start": "npm run mvp",
    "test": "npm run report"
  }
}
