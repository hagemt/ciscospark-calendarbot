{
  "name": "ciscospark-meetingbot",
  "version": "0.0.0-hackathon",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "bunyan": "1.8.12",
    "config": "1.30.0",
    "http-shutdown": "1.2.0",
    "koa": "2.5.0",
    "koa-omnibus": "0.9.3",
    "koa-router": "7.4.0",
    "koa-static": "4.0.2",
    "lodash": "4.17.5",
    "supertest": "3.0.0"
  },
  "devDependencies": {
    "eslint": "4.18.1",
    "eslint-plugin-import": "2.9.0",
    "mocha": "5.0.1",
    "nyc": "11.4.1"
  },
  "scripts": {
    "precoverage": "nyc -- npm run mocha",
    "coverage": "nyc --reporter=html report",
    "postcoverage": "nyc check-coverage",
    "eslint": "eslint .eslintrc.js config mvp test --report-unused-disable-directives",
    "lint": "npm run eslint",
    "mocha": "NODE_ENV=test mocha test/mvp.js",
    "mvp": "node mvp | bunyan",
    "prereport": "npm run lint -- --fix",
    "report": "npm run coverage",
    "postreport": "npm run lint -- --max-warnings 0",
    "start": "npm run mvp",
    "test": "npm run report"
  }
}
